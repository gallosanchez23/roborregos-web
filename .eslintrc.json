{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"airbnb-standard"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "flowtype"
    ],
    "rules": {
    }
}
